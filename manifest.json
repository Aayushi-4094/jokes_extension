{
    "manifest_version": 2,
    "name": "Jokes",
    "version": "0.0.1",
    "description": "A simple browser extension for jokes.",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "jokes.png"
        }
    },
    "icons": {
        "128": "jokes.png"
    }
}
